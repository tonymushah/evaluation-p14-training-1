{
	"name": "evaluation-p14-training",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"codegen": "graphql-codegen",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"storybook:dev": "storybook dev -p 6006",
		"storybook:build": "storybook build",
		"backend:magasin": "cargo run --bin magasin",
		"backend:point-vente": "cargo run --bin point_vente",
		"codegen:magasin": "pnpm backend:magasin && pnpm codegen -c ./app/codegen/magasin.ts",
		"codegen:point-vente": "pnpm backend:point-vente && pnpm codegen -c ./app/codegen/point-vente.ts",
		"codegen:all": "pnpm codegen:magasin && pnpm codegen:point-vente",
		"backend": "pnpm codegen:all && cargo run"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^1.4.0",
		"@faker-js/faker": "^8.4.1",
		"@graphql-codegen/cli": "^5.0.2",
		"@graphql-codegen/client-preset": "^4.2.6",
		"@graphql-typed-document-node/core": "^3.2.0",
		"@playwright/test": "^1.28.1",
		"@storybook/addon-essentials": "^8.1.1",
		"@storybook/addon-interactions": "^8.1.1",
		"@storybook/addon-links": "^8.1.1",
		"@storybook/addon-styling-webpack": "^1.0.0",
		"@storybook/addon-themes": "^8.1.1",
		"@storybook/blocks": "^8.1.1",
		"@storybook/svelte": "^8.1.1",
		"@storybook/sveltekit": "^8.1.1",
		"@storybook/test": "^8.1.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-node": "^5.0.1",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "^8.56.0",
		"@types/js-cookie": "^3.0.6",
		"@types/lodash": "^4.17.4",
		"@types/uuid": "^9.0.8",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"autoprefixer": "^10.4.19",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-storybook": "^0.8.0",
		"eslint-plugin-svelte": "^2.35.1",
		"graphql": "^16.8.1",
		"postcss": "^8.4.38",
		"postcss-import": "^16.1.0",
		"postcss-nesting": "^12.1.4",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"sass": "^1.77.2",
		"shadcn-svelte": "^0.10.6",
		"storybook": "^8.1.1",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"svelte-headless-table": "^0.18.2",
		"svelte2tsx": "^0.7.8",
		"tailwindcss": "^3.4.3",
		"tslib": "^2.4.1",
		"typescript": "^5.4.5",
		"vite": "^5.0.3",
		"vite-plugin-kit-routes": "^0.6.2",
		"vitest": "^1.2.0"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/source-sans-pro": "^5.0.8",
		"@urql/svelte": "^4.2.0",
		"clsx": "^2.1.1",
		"dotenv": "^16.4.5",
		"js-cookie": "^3.0.5",
		"lodash": "^4.17.21",
		"tailwind-merge": "^2.3.0",
		"tailwind-variants": "^0.2.1",
		"uuid": "^9.0.1"
	}
}
